<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.hubu.mall.dao.SpuInfoDao">
    <update id="updateSpuInfoStatusById">
        UPDATE pms_spu_info set publish_status = #{statusCode},update_time = NOW()
        where id = #{spuId}
    </update>
</mapper>
